{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "car": {
      "via": "manager",
      "model": "car"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "birthday": {
      "type": "date"
    },
    "point": {
      "type": "integer",
      "default": 0,
      "required": false
    },
    "orders": {
      "via": "customer",
      "collection": "order"
    },
    "shipments": {
      "via": "driver",
      "collection": "shipment"
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "storage": {
      "via": "store_managers",
      "model": "storage"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Admin",
        "Stocker",
        "Driver",
        "Assistance",
        "User",
        "Iron",
        "Gold",
        "Diamond"
      ]
    },
    "furloughs": {
      "via": "driver",
      "collection": "furlough"
    },
    "package_templates": {
      "via": "user",
      "collection": "package-template"
    },
    "order_templates": {
      "via": "user",
      "collection": "order-template"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "report": {
      "via": "stocker",
      "model": "report"
    }
  }
}
