{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fee": {
      "type": "decimal",
      "required": false,
      "default": 0
    },
    "remain_fee": {
      "type": "decimal",
      "required": false,
      "default": 0
    },
    "state": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "note": {
      "type": "text"
    },
    "sender_name": {
      "type": "string",
      "required": true
    },
    "sender_phone": {
      "type": "string",
      "required": true
    },
    "receiver_name": {
      "type": "string",
      "required": true
    },
    "receiver_phone": {
      "type": "string",
      "required": true
    },
    "rating_point": {
      "type": "integer",
      "min": 0
    },
    "rating_note": {
      "type": "text"
    },
    "voucher": {
      "model": "voucher"
    },
    "customer": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    },
    "packages": {
      "via": "order",
      "collection": "package"
    },
    "from_address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "to_address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "name": {
      "type": "string"
    },
    "payments": {
      "via": "order",
      "collection": "payment"
    }
  }
}
