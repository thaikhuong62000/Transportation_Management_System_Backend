{
  "kind": "collectionType",
  "collectionName": "vouchers",
  "info": {
    "name": "Voucher",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "sale": {
      "type": "decimal",
      "required": true
    },
    "sale_max": {
      "type": "float",
      "required": true
    },
    "expired": {
      "type": "datetime"
    },
    "sale_type": {
      "type": "enumeration",
      "enum": [
        "percentage",
        "value"
      ],
      "default": "percentage"
    },
    "minimum_order": {
      "type": "biginteger",
      "default": "0"
    },
    "voucher_img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "customer_type": {
      "type": "enumeration",
      "enum": [
        "All",
        "Iron",
        "Gold",
        "Diamond"
      ],
      "default": "All"
    },
    "description": {
      "type": "text"
    }
  }
}
