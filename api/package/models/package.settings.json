{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "name": "package",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "required": false
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "weight": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "state": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "order": {
      "via": "packages",
      "model": "order"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "current_address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "imports": {
      "via": "package",
      "collection": "import"
    },
    "exports": {
      "via": "package",
      "collection": "export"
    },
    "name": {
      "type": "string"
    },
    "size": {
      "type": "component",
      "repeatable": false,
      "component": "package.size"
    },
    "package_type": {
      "type": "component",
      "repeatable": false,
      "component": "package.package-type"
    },
    "shipments": {
      "via": "packages",
      "collection": "shipment"
    },
    "position": {
      "type": "string"
    }
  }
}
