{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "name": "Report",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "total_import": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "total_export": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "note": {
      "type": "text"
    },
    "stocker": {
      "via": "reports",
      "plugin": "users-permissions",
      "model": "user"
    },
    "report_file": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "storage": {
      "via": "reports",
      "model": "storage"
    }
  }
}
